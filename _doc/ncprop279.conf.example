# Namecoin RPC password
# namecoinrpcpassword=""

# Timeout (in milliseconds) for Namecoin RPC requests
# namecoinrpctimeout=1500

# Namecoin RPC username
# namecoinrpcusername=""

# Only return onion services? (default false)
# onlyonion=false
